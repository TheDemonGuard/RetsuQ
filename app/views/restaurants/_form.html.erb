<%= form_for(restaurant) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :category %>
  <%= f.text_field :category %>
  <%= f.label :price_range %>
  <%= f.text_field :price_range %>
  <%= f.label :open_time %>
  <%= f.number_field :open_time %>
  <%= f.label :close_time %>
  <%= f.number_field :close_time %>
  <%= f.label :status %>
  <%= f.text_field :status %>
  <%= f.label :capacity %>
  <%= f.number_field :capacity %>
  <%= f.label :total_wait_time %>
  <%= f.number_field :total_wait_time %>
  <%= f.label :time_per_person %>
  <%= f.number_field :time_per_person %>
  <%= f.submit %>
<% end %>
